{
  "name": "metamask-state-log-explorer",
  "version": "1.0.0",
  "description": "A boilerplate for react-hyperscript using browserify.",
  "main": "index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "yarn serve & yarn watch",
    "serve": "static-server . -p 9013 -z",
    "build": "yarn build:prep && yarn bundle",
    "watch": "yarn build -w",
    "bundle": "node build.js",
    "build:prep": "mkdir -p dist && rm -rf dist/*",
    "prepare": "yarn build"
  },
  "author": "Dan Finlay",
  "license": "ISC",
  "dependencies": {
    "ethereumjs-util": "^5.1.2",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-dropzone": "^4.1.3",
    "react-hyperscript": "^2.4.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.3.1",
    "redux-thunk": "^1.0.2",
    "xtend": "^4.0.1"
  },
  "keywords": [
    "react",
    "hyperscript",
    "browserify",
    "boilerplate"
  ],
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "brfs": "^2.0.2",
    "browserify": "^16.5.1",
    "static-server": "^2.2.1",
    "watchify": "^3.11.1"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MetaMask/state-log-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/MetaMask/state-log-explorer/issues"
  },
  "homepage": "https://github.com/MetaMask/state-log-explorer#readme"
}
